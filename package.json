{
	"name": "jobsys-newbie-next",
	"version": "0.1.1",
	"description": "AI-friendly React components built with Ant Design 6.1",
	"keywords": [
		"ai-friendly",
		"ant-design",
		"antd",
		"form",
		"pro-components",
		"react",
		"search",
		"table",
		"typescript"
	],
	"license": "ISC",
	"author": "sinceow@163.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/jobsys/newbie-next"
	},
	"files": [
		"CHANGELOG.md",
		"dist",
		"README.md"
	],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./styles": "./dist/styles.css"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc -b --verbose && vite build",
		"preview": "vite preview",
		"lint": "oxlint src",
		"lint:fix": "oxlint --fix src",
		"format": "oxfmt",
		"format:check": "oxfmt --check",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "pnpm build && changeset publish",
		"prepare": "husky"
	},
	"dependencies": {
		"@ant-design/icons": "^6.1.0",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"axios": "^1.13.2",
		"dayjs": "^1.11.13",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.562.0"
	},
	"devDependencies": {
		"@ant-design/cssinjs": "^1.22.1",
		"@ant-design/pro-components": "^2.8.4",
		"@changesets/cli": "^2.27.9",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.9.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.3",
		"@vitest/ui": "^4.0.16",
		"antd": "^5.24.0",
		"husky": "^9.1.7",
		"jsdom": "^27.3.0",
		"lint-staged": "^16.2.7",
		"oxfmt": "^0.19.0",
		"oxlint": "^1.33.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.11",
		"vite-plugin-dts": "^4.3.0",
		"vite-plugin-progress": "^0.0.7",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"@ant-design/pro-components": "^2.0.0",
		"antd": "^5.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
			"oxfmt"
		]
	}
}
